{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-react-hooks"
  ],
  "rules": {
    "react-hooks-nesting": "error",
    "no-console": false,
    "import-name": false,
    "semicolon": false,
    "ter-arrow-parens": [
      true,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "trailing-comma": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case"
    ],
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "interface-name": false,
    "max-line-length": false,
    "jsx-wrap-multiline": false
  }
}